# Runyx Sync Agent - Environment Variables
# Copy this file to .env and fill in your values

# Agent authentication token (REQUIRED)
# Get this from the Runyx Admin Panel when enabling Hybrid Mode
AGENT_TOKEN=your-agent-token-here

# Tenant ID (REQUIRED)
# Your organization's tenant ID from the Admin Panel
TENANT_ID=your-tenant-id-here

# Agent ID (REQUIRED)
# Unique identifier for this agent instance
AGENT_ID=your-agent-id-here

# Cloud WebSocket URL (OPTIONAL)
# Default: wss://dataguard.runyx.io/ws
AGENT_CLOUD_URL=wss://dataguard.runyx.io/ws

# Log level (OPTIONAL)
# Options: debug, info, warn, error
# Default: info
AGENT_LOG_LEVEL=info
